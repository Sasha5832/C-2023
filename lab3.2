        int Minimum (int[] tab, int start, int end)
        {
            int wynik = start;
            for(int i=start; i <end; i++)
            {
                if(tab[i] < tab[wynik])
                {
                    wynik = i;
                }
            }
            return wynik;
        }
        int[] Covert (string napis)
        {
            var liczbyS = napis.Trim().Split('_');
            int[] liczby =new int[liczbyS.Length];
            for(int i =0; i<liczbyS.Length; i++)
            {
                liczby[i] = int.Parse(liczbyS[i]);
            }
            return liczby;
        }
        int Fib(int n)
        {
            if (n == 1)
            {
                return 1;
            }
            else if (n == 0)
            {
                return 0;
            }
            return Fib(n - 1)+Fib(n-2);
        }
        ulong Fib2(int n)
        {
            if (n == 0 || n == 1)
            {
                return (ulong)n;
            }
            ulong[] wyrazy = new ulong[n+1];
            wyrazy[0] = 0;
            wyrazy[1] = 1;
            for (int i = 2; i < wyrazy.Length; i++)
            {
                wyrazy[i] = wyrazy[i - 1] + wyrazy[i - 2];
            }
            return wyrazy[n];
        }
